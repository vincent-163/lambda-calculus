Grp=
  T: *
  Grp. *
  Fns.
    id. T
    op. (.T .T T)
    Grp
  Grp

Grp_Id=
  T: *
  Grp: (Grp T)
  (Grp (.T .T T)
    (id: T
     op: (.T .T T)
     id))

Grp_Op=
  T: *
  Grp: (Grp T)
  (Grp (.T .T T)
    (id: T
     op: (.T .T T)
     op))

Nat=
  Nat. *
  Succ. (.Nat Nat)
  Zero. Nat
  Nat

zero=
  Nat: *
  Succ: (.Nat Nat)
  Zero: Nat
  Zero

one=
  Nat: *
  Succ: (.Nat Nat)
  Zero: Nat
  (Succ Zero)

two=
  Nat: *
  Succ: (.Nat Nat)
  Zero: Nat
  (Succ (Succ Zero))

mul=
  a: Nat
  b: Nat
  Nat: *
  Succ: .Nat Nat
  Zero: Nat
  (a Nat (b Nat Succ) Zero)

exp=
  a: Nat
  b: Nat
  Nat: *
  (b (.Nat Nat) (a Nat))

NatGrp=
  Grp: *
  Fns:
    id. Nat
    op. (.Nat .Nat Nat)
    Grp
  (Fns zero mul)

(Grp_Op Nat NatGrp one two)
